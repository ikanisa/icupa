# OpenAI Realtime API
OPENAI_API_KEY=sk-...
OPENAI_REALTIME_MODEL=gpt-4o-realtime-preview-2024-12-17
OPENAI_REALTIME_VOICE=verse

# Twilio
TWILIO_ACCOUNT_SID=AC...
TWILIO_AUTH_TOKEN=...
TWILIO_FROM_NUMBER=+1...
# Optional: Twilio webhook authentication
TWILIO_WEBHOOK_AUTH=shared-secret

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=...

# Public URLs
PUBLIC_WS_URL=wss://your-tunnel-host.trycloudflare.com/ws/twilio
PUBLIC_HTTP_URL=https://your-tunnel-host.trycloudflare.com

# Cloudflare Tunnel
CLOUDFLARE_TUNNEL_TOKEN=...

# Server configuration
PORT=8787
MCP_PORT=9797

# Feature flags
VOICE_AGENT_ENABLED=true
VOICE_AGENT_DUPLEX_ENABLED=false
VOICE_AGENT_MCP_ENABLED=true
VOICE_AGENT_STORAGE_ENABLED=true

# Logging
LOG_LEVEL=info
NODE_ENV=development
