# Admin Console environment variables
# Copy to .env.local and provide real values before running locally or deploying.

# Public Supabase credentials shared across browser bundles
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=supabase-anon-key
NEXT_PUBLIC_AGENTS_URL=https://agents.icupa.dev

# Optional metadata for generating absolute links
NEXT_PUBLIC_APP_URL=https://admin.icupa.dev

# Server-only secrets (never expose to browser clients)
SUPABASE_SERVICE_ROLE_KEY=supabase-service-role-key
