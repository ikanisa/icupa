{
  "name": "@icupa/config",
  "version": "0.1.0",
  "private": true,
  "description": "Shared configuration presets for the ICUPA monorepo.",
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "files": [
    "src",
    "tailwind-preset.ts",
    "eslint",
    "prettier",
    "tsconfig"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./env": "./src/env.ts",
    "./feature-flags": "./src/feature-flags.ts",
    "./tailwind-preset": "./tailwind-preset.ts",
    "./primitives": "./src/primitives/index.ts",
    "./primitives/*": "./src/primitives/*",
    "./*": "./src/*",
    "./eslint": "./eslint/index.mjs",
    "./prettier": "./prettier/index.mjs",
    "./tsconfig/base": "./tsconfig/base.json",
    "./tsconfig/next": "./tsconfig/next.json",
    "./tsconfig/node": "./tsconfig/node.json"
  },
  "dependencies": {
    "zod": "^4.1.12"
  },
  "peerDependencies": {
    "@icupa/types": "workspace:*",
    "@eslint/js": "^9.32.0",
    "@types/node": "^24.9.2",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "prettier": "^3.5.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  }
}
