# Copy to oauth2-proxy.env and configure your identity provider integration.
OAUTH2_PROXY_PROVIDER=google
OAUTH2_PROXY_CLIENT_ID=
OAUTH2_PROXY_CLIENT_SECRET=
OAUTH2_PROXY_COOKIE_SECRET=
OAUTH2_PROXY_EMAIL_DOMAINS=icupa.dev
OAUTH2_PROXY_REDIRECT_URL=https://${AGENTS_PUBLIC_HOST}/oauth2/callback
OAUTH2_PROXY_COOKIE_NAME=_icupa_agents_auth
OAUTH2_PROXY_COOKIE_SAMESITE=lax
OAUTH2_PROXY_COOKIE_REFRESH=12h
OAUTH2_PROXY_COOKIE_EXPIRE=24h
OAUTH2_PROXY_WHITELIST_DOMAINS=${AGENTS_PUBLIC_HOST}
OAUTH2_PROXY_SKIP_PROVIDER_BUTTON=true
