{
  "name": "icupa-monorepo",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.4.0",
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*",
    "agents-service"
  ],
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "dev:client": "pnpm --filter @ecotrips/client dev",
    "dev:admin": "pnpm --filter @ecotrips/admin dev",
    "dev:supplier": "pnpm --filter @ecotrips/supplier dev",
    "build:all": "turbo run build --cache-dir .turbo",
    "dev:agents": "npm --prefix agents-service run dev",
    "dev:voice-agent": "pnpm --filter @icupa/voice-agent dev",
    "prebuild": "node scripts/ci/check-supabase-env.mjs",
    "pretest": "node scripts/ci/reset-supabase-test-db.mjs",
    "build": "tsc -b && vite build",
    "build:dev": "vite build --mode development",
    "lint": "pnpm lint:repo",
    "lint:repo": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:all": "turbo run lint --cache-dir .turbo",
    "typecheck": "tsc -b --pretty false",
    "typecheck:all": "turbo run typecheck --cache-dir .turbo",
    "preview": "vite preview --port 5000",
    "dev:ocr:converter": "node apps/ocr-converter/server.mjs",
    "dev:all": "bash scripts/dev/dev-all.sh",
    "smoke:ingestion": "node scripts/smoke/ingestion-smoke.mjs",
    "release:canary-toggle": "node scripts/release/canary-toggle.mjs",
    "release:checklist": "node scripts/release/checklist.mjs",
    "supabase:start": "npx --yes supabase start",
    "supabase:stop": "npx --yes supabase stop",
    "supabase:status": "npx --yes supabase status",
    "supabase:migrate": "npx --yes supabase migration up",
    "supabase:reset": "npx --yes supabase db reset",
    "supabase:test": "npx --yes supabase db test",
    "test": "vitest run",
    "test:all": "turbo run test --cache-dir .turbo",
    "test:mcp": "vitest run tests/mcp",
    "test:accessibility": "vitest run tests/accessibility",
    "security:lint-mcp": "node scripts/security/lint-mcp-tools.mjs",
    "pretest:e2e": "node scripts/ci/reset-supabase-test-db.mjs",
    "test:e2e": "playwright test --config tests/playwright/playwright.config.ts",
    "test:perf": "npx --yes @lhci/cli@0.15.1 autorun",
    "analyze:pwa": "npx --yes lighthouse http://localhost:5000 --only-categories=pwa --preset=desktop || true",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "verify": "pnpm run lint:all && pnpm run typecheck:all && pnpm run test:all",
    "verify:full": "pnpm run verify && pnpm run test:e2e && pnpm run supabase:test",
    "presupabase:test": "node scripts/ci/reset-supabase-test-db.mjs"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@opentelemetry/exporter-trace-otlp-http": "^0.207.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.207.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@supabase/supabase-js": "^2.78.0",
    "@tanstack/react-query": "^5.83.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.22",
    "fuse.js": "^7.1.0",
    "immer": "^10.1.3",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.462.0",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-day-picker": "^9.11.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.61.1",
    "react-resizable-panels": "^3.0.6",
    "react-router-dom": "^6.30.1",
    "recharts": "^2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.9",
    "web-vitals": "^4.2.4",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@icupa/config": "workspace:*",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.32.0",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.9.2",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "dotenv": "^16.6.1",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "prettier": "^3.5.1",
    "supabase": "^2.53.6",
    "tailwindcss": "^3.4.17",
    "turbo": "^2.5.8",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^5.4.19",
    "vitest": "^4.0.5",
    "vitest-axe": "^0.1.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,cjs,mjs}": [
      "eslint --max-warnings=0",
      "prettier --write"
    ],
    "*.{json,md,css,scss,html,graphql,yml,yaml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.10.0 <21"
  }
}
